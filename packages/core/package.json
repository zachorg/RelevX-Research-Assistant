{
  "name": "core",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./backend": {
      "types": "./dist/index-backend.d.ts",
      "import": "./dist/index-backend.js",
      "require": "./dist/index-backend.js",
      "default": "./dist/index-backend.js"
    },
    "./web": {
      "types": "./dist/index-web.d.ts",
      "import": "./dist/index-web.js",
      "require": "./dist/index-web.js",
      "default": "./dist/index-web.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/cheerio": "^1.0.0",
    "cheerio": "^1.1.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "firebase": "^10.7.1",
    "firebase-admin": "^12.0.0",
    "js-yaml": "^4.1.1",
    "marked": "^17.0.1",
    "openai": "^6.9.0",
    "react": "^18.2.0",
    "resend": "^6.5.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^18.2.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.3.0"
  }
}
